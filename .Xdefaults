!! dark bluish gvim background
#define S_base0         #1e1e27
!! normal text color
#define S_base1         #cfbfad
#define S_base2         #ff0000
!! bright green as in the bar
#define S_base3         #afd787
!! light green as in the bar
#define S_base4         #9fad8c
!! grey as in the bar
#define bar_grey        #3a3a3a

*background:            S_base0
*foreground:            S_base1
!! what is this?
*fadeColor:             S_base2
*cursorColor:           S_base3
*pointerColorBackground:S_base1
*pointerColorForeground:S_base01

!Black
URxvt.color0:     bar_grey
URxvt.color8:     #707172
!Red
URxvt.color1:     #cc6666
URxvt.color9:     #ce4e4e
!Green
URxvt.color2:     S_base3
URxvt.color10:    S_base4
!Yellow
URxvt.color3:     #ffcd8b
URxvt.color11:    #ffcd00
!Blue
URxvt.color4:     #7e7eae
URxvt.color12:    #6e6eaf
!Magenta
URxvt.color5:     #ff8bff
URxvt.color13:    #c080d0
!Cyan
URxvt.color6:     #808bed
URxvt.color14:    #66cccc
!White
URxvt.color7:     #ffffff
URxvt.color15:    #eeeeee

!!urxvt*font:           xft:Monospace:size=10
urxvt*font:             xft:DejaVu Sans Mono:size=10
